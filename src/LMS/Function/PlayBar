import styled from "styled-components";

const Container = styled.div``;
export function PlayBar() {
  const { playing, setPlaying } = props;
  const [isModalOpen, setIsModalOpen] = useState(false);

  const handlePlayBtn = () => {
    if (playing == false) {
      setPlaying(true);
    } else {
      setPlaying(false);
    }
  };
  return (
    <>
      <Container></Container>
    </>
  );
}
